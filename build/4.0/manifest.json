{
    "background" : {
        "page" : "/html/lmk123.html" ,
        "persistent" : false
    } ,
    "browser_action" : {
        "default_icon" : {
            "19" : "/img/logo.png" ,
            "38" : "/img/logo.png"
        } ,
        "default_popup" : "/html/popup.html" ,
        "default_title" : "翻译盒子"
    } ,
    "commands" : {
        "_execute_browser_action" : {
            "suggested_key" : {
                "default" : "Ctrl+Q"
            }
        }
    } ,
    "content_scripts" : [
        {
            "matches" : ["<all_urls>"] ,
            "js" : ["/js/common.js", "/js/content.js"] ,
            "run_at" : "document_start" ,
            "all_frames" : true
        }
    ] ,
    "content_security_policy" : "script-src 'self' https://ssl.google-analytics.com; object-src 'self'" ,
    "description" : "整合了百度与有道的在线翻译服务，支持中、日、英等语言的互译及右键与划词翻译，同时增加了有道的网页翻译功能。" ,
    "homepage_url" : "http://blog.lmk123.com/?p=30" ,
    "icons" : {
        "128" : "/img/logo.png" ,
        "16" : "/img/ico16.png" ,
        "48" : "/img/logo.png"
    } ,
    "manifest_version" : 2 ,
    "author" : "Milk Lee( i@lmk123.com )" ,
    "short_name" : "RushOutAsia" ,
    "name" : "右键翻译" ,
    "permissions" : [
        "<all_urls>", "contextMenus", "storage", "cookies", "unlimitedStorage", "clipboardWrite", "clipboardRead",
        "idle", "notifications"
    ] ,
    "version" : "4.0" ,
    "web_accessible_resources" : ["/js/youdaoweb.js"]
}
